
<div class="block_branding col-xs-6 col-sm-4 col-md-3 col-lg-2">


{% block content %}
  {% if site_logo %}
    <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home" class="site-logo with-tagline">
      <img src="{{ site_logo }}" alt="{{ 'Home'|t }}" />
    </a>
    <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home" class="site-logo no-tagline">
      <img src="/themes/contrib/franklin/logo_white.svg" alt="{{ 'Home'|t }}" />
    </a>
  {% endif %}
  {% if site_name %}
    <div class="site-name">
      <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">{{ site_name }}</a>
    </div>
  {% endif %}
  {% if site_slogan %}
    <div class="site-slogan">{{ site_slogan }}</div>
  {% endif %}
{% endblock %}
</div>
